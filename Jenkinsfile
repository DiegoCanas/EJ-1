pipeline {
    agent any
    stages {
        stage ('Checkout'){
            steps{
                git 'Url al repo'
            }
        }

        stage ('Instalacion de dependencias'){
            steps{
                sh 'npm install'
            }

        }

        stage ('Linteo'){
            steps{
                echo("hola")
            }

        }

        stage ('Test'){
            steps{
                echo("hola")
            }

        }

        stage ('Build'){
            steps{
                echo("hola")
            }

        }
    }
}